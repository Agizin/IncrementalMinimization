Alphabet Node[x:int,y:int,s:string]{Three(3),One(1),Zero(0),Cons(2),Nil(0)}
Lang fastacc_q8 : Node {
	  Zero() where true
}
Lang fastacc_q2 : Node {
	  One(x1) where true given (fastacc_q8 x1)
}
Lang fastacc_q4 : Node {
	  Three(x1,x2,x3) where true given (fastacc x1)(fastacc_q4 x2)(fastacc_q2 x3)
	| Three(x1,x2,x3) where (x > 7501) given (fastacc_q8 x1)(fastacc_q4 x2)(fastacc_q4 x3)
	| One(x1) where (6182 == x) given (fastacc_q2 x1)
}
Public Lang fastacc : Node {
	  Three(x1,x2,x3) where true given (fastacc_q2 x1)(fastacc_q4 x2)(fastacc_q2 x3)
	| Three(x1,x2,x3) where ((x <= 5748) and (3033 == y)) given (fastacc x1)(fastacc x2)(fastacc_q2 x3)
}
