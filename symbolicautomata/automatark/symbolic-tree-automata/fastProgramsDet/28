Alphabet Node[x:int,y:int,s:string]{Three(3),One(1),Zero(0),Cons(2),Nil(0)}
Lang fastacc_q10 : Node {
	  Zero() where true
}
Lang fastacc_q4 : Node {
	  Three(x1,x2,x3) where true given (fastacc_q10 x1)(fastacc_q10 x2)(fastacc_q10 x3)
	| Three(x1,x2,x3) where (9848 <= x) given (fastacc x1)(fastacc_q10 x2)(fastacc_q4 x3)
	| Three(x1,x2,x3) where (7700 <= y) given (fastacc x1)(fastacc_q16 x2)(fastacc_q4 x3)
	| Three(x1,x2,x3) where true given (fastacc_q4 x1)(fastacc_q4 x2)(fastacc_q16 x3)
	| Three(x1,x2,x3) where (7700 <= y) given (fastacc x1)(fastacc_q20 x2)(fastacc_q4 x3)
	| Three(x1,x2,x3) where true given (fastacc_q20 x1)(fastacc_q4 x2)(fastacc_q16 x3)
	| Three(x1,x2,x3) where true given (fastacc_q4 x1)(fastacc_q20 x2)(fastacc_q16 x3)
	| Three(x1,x2,x3) where true given (fastacc_q20 x1)(fastacc_q20 x2)(fastacc_q16 x3)
	| Three(x1,x2,x3) where (9848 <= x) given (fastacc x1)(fastacc_q10 x2)(fastacc_q20 x3)
	| Three(x1,x2,x3) where true given (fastacc_q4 x1)(fastacc_q4 x2)(fastacc_q20 x3)
	| Three(x1,x2,x3) where true given (fastacc_q20 x1)(fastacc_q4 x2)(fastacc_q20 x3)
	| Three(x1,x2,x3) where (7700 <= y) given (fastacc x1)(fastacc_q16 x2)(fastacc_q20 x3)
	| Three(x1,x2,x3) where true given (fastacc_q4 x1)(fastacc_q20 x2)(fastacc_q20 x3)
	| Three(x1,x2,x3) where (7700 <= y) given (fastacc x1)(fastacc_q20 x2)(fastacc_q20 x3)
	| Three(x1,x2,x3) where true given (fastacc_q20 x1)(fastacc_q20 x2)(fastacc_q20 x3)
	| One(x1) where (3682 < y) given (fastacc x1)
}
Public Lang fastacc : Node {
	  Three(x1,x2,x3) where true given (fastacc_q10 x1)(fastacc_q4 x2)(fastacc_q10 x3)
	| Three(x1,x2,x3) where true given (fastacc_q10 x1)(fastacc_q20 x2)(fastacc_q10 x3)
}
Lang fastacc_q16 : Node {
	  Three(x1,x2,x3) where true given (fastacc_q10 x1)(fastacc x2)(fastacc x3)
}
Lang fastacc_q20 : Node {
	  One(x1) where (3682 >= y) given (fastacc x1)
}
